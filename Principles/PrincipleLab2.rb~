#Tokenizer Class
class Tokenizer
  def initialize(unparsed)
    @token_list = []
    @token_types = []
    @token = ""
    @unparsed_string = unparsed
  end

	def tokenize
		index = 0

		while index < @unparsed_string.length()-1 do
			if @unparsed_string[index] == ""

			elsif @unparsed_string[index] == ""

			elsif @unparsed_string[index] == ""

			end
			
		end

	end

end
